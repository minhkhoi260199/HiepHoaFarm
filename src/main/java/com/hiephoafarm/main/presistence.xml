<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">
      <class>com.hiephoafarm.main.models.AdsE</class>
    <class>com.hiephoafarm.main.models.CategoryE</class>
    <class>com.hiephoafarm.main.models.GalleryE</class>
    <class>com.hiephoafarm.main.models.OrderDetailE</class>
    <class>com.hiephoafarm.main.models.OrdersE</class>
    <class>com.hiephoafarm.main.models.ProductE</class>
    <class>com.hiephoafarm.main.models.RoleE</class>
    <class>com.hiephoafarm.main.models.StatusE</class>
    <class>com.hiephoafarm.main.models.UserE</class>
      <class>com.hiephoafarm.main.models.GalleryObj</class>
      <class>com.hiephoafarm.main.models.ProductObj</class>
      <properties>
      <property name="hibernate.connection.url" value="jdbc:mariadb://localhost:3306/hiephoafarm"/>
      <property name="hibernate.connection.driver_class" value="org.mariadb.jdbc.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
